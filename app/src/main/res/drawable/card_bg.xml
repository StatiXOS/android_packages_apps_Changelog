<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle"
			android:dither="true">
			<corners android:radius="1dp"/>
			<solid android:color="@color/changelog_background_shade_color" />
		</shape>
	</item>
	<item android:bottom="1dp">
		<shape android:shape="rectangle"
			android:dither="true">
			<corners android:radius="1dp" />
			<solid android:color="@color/changelog_background_color" />
			<padding android:bottom="8dp"
				android:left="8dp"
				android:right="8dp"
				android:top="8dp" />
		</shape>
	</item>
</layer-list>
